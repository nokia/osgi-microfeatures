# Feature for swagger support

Bundle-Version: 1.0.0
Bundle-Name: Swagger

Private-Package:

Require-Capability: \
  osgi.identity;filter:='(osgi.identity=com.nokia.as.http.swagger.core)'

Provide-Capability: com.nokia.as.feature;\
  com.nokia.as.feature="lib.csf.cswg";\
  version:Version="1.0.0";\
  desc="Swagger enabling easy access to your REST API"

