{
	"$schema": "http://json-schema.org/draft-03/schema",
	"type": "object",
	"javaType": "com.nsn.ood.cls.model.gen.clients.Clients",
	"description": "Client reservation content type schema",
	"extends": {
		"$ref": "resource.json"
	},
	"properties": {
		"clients": {
			"type": "array",
			"required": true,
			"description": "List of clients",
			"items": {
				"type": "object",
				"javaType": "com.nsn.ood.cls.model.gen.clients.Client",
				"required": false,
				"properties": {
					"clientId": {
						"type": "string",
						"required": false,
						"description": "Unique client ID - generated by CLS or provided by client"
					},
					"targetType": {
						"type": "string",
						"required": false,
						"description": "License Target NE Type, i.e. WBTS"
					},
					"keepAliveTime": {
						"type": "integer",
						"required": false,
						"description": "Keep alive time in seconds, server configuration value used if empty"
					},
					"features": {
						"$ref": "features.json#/properties/features"
					},
					"errorFeatures": {
						"type": "array",
						"required": false,
						"description": "List error features",
						"default": null,
						"items": {
							"$ref": "error-feature.json"
						}
					}
				},
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}
