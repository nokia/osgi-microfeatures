	osgi.identity;filter:='(&(osgi.identity=slf4j.api)(version>=1.7.0))',\
	osgi.identity;filter:='(&(osgi.identity=slf4j.log4j12)(version>=1.7.0))',\
	osgi.identity;filter:='(&(osgi.identity=org.apache.felix.bundlerepository)(version>=2.0.10))',\
	osgi.identity;filter:='(osgi.identity=com.nokia.as.service.bundlerepository)'
	
-runfw: org.apache.felix.framework
-runee: JavaSE-1.8

-runproperties: \
	obr.remote=file:///home/nxuser/.m2/repository/obr.xml
	