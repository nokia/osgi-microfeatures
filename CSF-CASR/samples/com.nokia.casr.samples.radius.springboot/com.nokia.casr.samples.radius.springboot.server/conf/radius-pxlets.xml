<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE context PUBLIC "-//NEXTENSO//DTD CONTEXT 1.1//EN" "http://www.nextenso.com/">

<context>
  <name>Radius Proxylets</name>

  <acct-request-chain>
  </acct-request-chain>

  <auth-request-chain>
    <proxylet>
      <name>Auth Request Pxlet</name>
      <protocol>RADIUS</protocol>
      <class>com.nokia.casr.samples.radius.springboot.server.TestAccessRequestProxylet</class>
      <criterion>
	<name>all</name>
	<criterion-value>
	  <all />
	</criterion-value>
      </criterion>
    </proxylet>
  </auth-request-chain>

</context>
