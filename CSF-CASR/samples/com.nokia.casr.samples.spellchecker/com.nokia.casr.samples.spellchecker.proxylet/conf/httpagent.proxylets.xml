<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE context PUBLIC "-//NEXTENSO//DTD CONTEXT 1.1//EN" "http://www.nextenso.com/">

<context>
	<name>HTTP deployed context</name>
	<description>This is the deployed context for HTTP proxylets</description>

	<request-chain>
		<proxylet setid="demo">
			<name>SpellChecker</name>
			<protocol>HTTP</protocol>
			<class>com.nokia.casr.samples.spellchecker.proxylet.SpellCheckerProxylet</class>
			<criterion>
				<name>all</name>
				<criterion-value>
					<path>/spellcheck*</path>
				</criterion-value>
			</criterion>
		</proxylet>
	</request-chain>

	<response-chain>
	</response-chain>

</context>
