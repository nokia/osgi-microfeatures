<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE context PUBLIC "-//NEXTENSO//DTD CONTEXT 1.1//EN" "http://www.nextenso.com/">

<context>
        <name>DIAMETER context</name>
        <request-chain>

        <proxylet setid="Set_1">
                <name>TestServer</name>
                <description>A Test Server</description>
                <protocol>DIAMETER</protocol>
                <class>com.nokia.casr.samples.diameter.server.TestServer</class>
		<param>
		  <name>myparam</name>
		  <value>myparam</value>
		</param>
                <criterion>
		<!-- this is a way to filter requests / not used here -->
                        <name>AllRequests</name>
                        <criterion-value>
                                <all/>
                        </criterion-value>
                </criterion>
        </proxylet>

        </request-chain>

        <response-chain>
	  <!-- no response proxylet -->
        </response-chain>

</context>

