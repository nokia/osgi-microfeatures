Bundle-Name: SessionAPI
X-Obsoletes: SessionAPI
Bundle-Category: core, api
Export-Package:com.alcatel.as.session.distributed;\
 com.alcatel.as.session.distributed.event;\
 com.alcatel.as.session.distributed.message;\
 com.alcatel.as.session.distributed.smartkey;\
 com.alcatel.as.session.distributed.transaction;\
